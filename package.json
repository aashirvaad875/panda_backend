{
  "name": "panda-backend",
  "version": "1.0.0",
  "description": "panda web application",
  "main": "index.js",
  "scripts": {
    "dev:backend": "NODE_ENV=development ts-node-dev --ignore-watch node_modules  ./src/apps/panda/backend/start.ts",
    "test": "npm run test:unit && npm run test:features",
    "test:unit": "NODE_ENV=test jest",
    "test:features": "npm run test:backend:features",
    "test:backend:features": "NODE_ENV=test cucumber-js -p panda_backend",
    "tsc": "tsc",
    "check:types": "tsc --pretty --noEmit",
    "check:lint": "eslint . --ext ts,tsx,js,jsx",
    "check:all": "run-s check:lint check:types",
    "build:tsc": "tsc -p tsconfig.prod.json",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:run": "NODE_PATH=./src ts-node ./node_modules/typeorm/cli.js migration:run --config ./src/Contexts/Shared/infrastructure/persistance/ormconfig.ts",
    "migration": " npm run typeorm migration:run -- -d ./src/Contexts/Shared/infrastructure/entity/*.ts",
    "rabbit:config:command": "ts-node-dev  src/apps/panda/backend/command/runConfigureRabbitMQCommand",
    "prepare": "husky || true",
    "lint": "eslint --quiet src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "check-types": "tsc -p tsconfig.json --skipLibCheck --noEmit",
    "prettier": "prettier --write src/**/*.ts",
    "prettier:check": "prettier --check  src/**/*.ts"
  },
  "engines": {
    "node": ">=20.4.0",
    "npm": ">=9.7.2"
  },
  "author": "ashik chalise",
  "license": "ISC",
  "devDependencies": {
    "@cucumber/cucumber": "^10.1.0",
    "@types/amqplib": "^0.10.4",
    "@types/cli-table": "^0.3.4",
    "@types/compression": "^1.7.5",
    "@types/convict": "^6.1.6",
    "@types/cors": "^2.8.17",
    "@types/cucumber": "^7.0.0",
    "@types/express": "^4.17.21",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.5.11",
    "@types/supertest": "^6.0.2",
    "@types/uuid-validate": "^0.0.3",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^9.0.7",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@types/app-root-path": "^1.2.8",
    "@types/errorhandler": "^1.5.3",
    "@types/glob": "^8.1.0",
    "@types/morgan": "^1.9.9",
    "amqplib": "^0.10.3",
    "app-root-path": "^3.1.0",
    "body-parser": "^1.20.2",
    "class-validator": "^0.14.1",
    "cli-table": "^0.3.11",
    "compression": "^1.7.4",
    "convict": "^6.2.4",
    "copy": "^0.3.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "errorhandler": "^1.5.1",
    "express": "^4.18.2",
    "express-promise-router": "^4.1.1",
    "express-validator": "^7.0.1",
    "glob": "^10.3.10",
    "helmet": "^7.1.0",
    "http-status": "^1.7.3",
    "mongodb": "^5.9.2",
    "morgan": "^1.10.0",
    "node-dependency-injection": "^2.6.11",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.2.1",
    "tslint": "^6.1.3",
    "typeorm": "^0.3.19",
    "uuid": "^9.0.1",
    "uuid-random": "^1.3.2",
    "uuid-validate": "^0.0.3",
    "winston": "^3.11.0"
  }
}
